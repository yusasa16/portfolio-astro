{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@libs/*": ["src/libs/*"],
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@types/*": ["src/types/*"],
			"@styles/*": ["src/styles/*"],
			"@styled-system/*": ["styled-system/*"]
		}
	}
}
